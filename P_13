//Write a C++ program to demonstrate use of Destructor.

#include<iostream>
using namespace std;

class sample
{
    public:
        sample()
        {
            cout<<"This is constructor sample class"<<endl;;
        }
        ~sample()
        {
            cout<<"This is destructor of class sample"<<endl;
        }
};

int main()
{
    cout<<"Purohit Akruti K."<<endl;
    cout<<"220130318033"<<endl;
    
    sample s1;
    return 0;
}
