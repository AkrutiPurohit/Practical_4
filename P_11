//Write a C++ program to demonstrate use of Parameterized Constructor.

#include<iostream>
using namespace std;

class demo
{
    public:
        demo(string n)
        {
            cout<<n <<" this perameterized constructor of class demo."<<endl;
        }
};

int main()
{
    cout<<"Purohit Akruti K."<<endl;
    cout<<"220130318033"<<endl;
    
    string name = "Akruti";
    demo d(name);
    
    return 0;
}
